<div id="ludimoodle-settings" class="ludimoodle-world-{{ world }}">
    <div class="row justify-content-center">
        <div class="col-6 block_section">
            <img class="ludimoodle-img img-responsive d-block mx-auto"
                 src="{{globals.config.wwwroot}}/course/format/ludimoodle/pix/{{ world }}/none.svg"
                 alt="Image not gamified">
        </div>
    </div>
    <p class="align-center">{{#str}} settings:nogamifieddescription, format_ludimoodle {{/str}}</p>
    <p class="align-center"><b>{{#str}} settings:nosetting, format_ludimoodle {{/str}}</b></p>
    {{> format_ludimoodle/settings_footer}}
</div>